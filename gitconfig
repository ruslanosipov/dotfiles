[color]
	ui = true
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[core]
	editor = vim
	autocrlf = false
	excludesfile = /usr/local/google/home/ruslano/.gitignore
[push]
	default = current
[alias]
	st = status
	cm = commit -m
	p = push
	pr = pull --rebase
	sh = stash
	sp = stash pop
	sa = stash apply
	d = difftool
	cp = cherry-pick
	b = branch
	show-files = "!f() { git diff-tree --no-commit-id --name-only -r $1; }; f"
	show-diff = "!f() { git difftool $1^ $1; }; f"
	pretty-log = log --graph --pretty=format:\"%Cred%h%Creset - %s %Cgreen(%cr) %C(bold blue)<%an>%Creset%C(yellow)%d%Creset\" --abbrev-commit --date=relative
[merge]
	tool = vimdiff
	conflictstyle = diff3
[mergetool]
	prompt = false
[include]
	path = ~/.gitconfig.local
[google]
	uplink = false
